<?xml version="1.0" encoding="ISO-8859-1" ?>
<sparrow-id-request
  xmlns="http://www.usgs.gov/sparrow/id-point-request/v0_2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<!-- The user now has a prediction context, so use it. -->
	<prediction-context context-id="389392"/>
	
	<!-- We now know the ID of the reach, so use it. -->
	<reach id="15"/>
	
	<!-- Ask for just the info on the 2nd tab. -->
	<content>
		<predicted/>
	</content>
	
	<!-- Get the response back as JSON -->
	<response-format>
		<mime-type>application/json</mime-type>
	</response-format>
</sparrow-id-request>